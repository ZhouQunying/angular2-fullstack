// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "eslint-config-airbnb-es5",
  "rules": {
    "func-names": 0,
    "camelcase": [2, {"properties": "never"}],
    "no-else-return": 0,
    "no-return-assign": 0,
    "one-var": 0
  },
  "globals": {
    "angular": false,
    "_": false
  }
}
